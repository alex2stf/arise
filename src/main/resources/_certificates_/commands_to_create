keytool -genkey -keyalg RSA -alias selfsigned -keystore keystore.jks -storepass astox_password -validity 360 -keysize 2048
#pass: astox_password


keytool -list -keystore keystore.jks
keytool -importkeystore -srckeystore keystore.jks -destkeystore keystore.p12 -deststoretype PKCS12

#setup source and destination password





